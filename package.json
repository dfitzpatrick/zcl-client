    "@electron-forge/maker-zip": "^6.0.0-beta.49",
    "@electron-forge/publisher-github": "^6.0.0-beta.51",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "electron": "8.0.1",
